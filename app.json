{
  "expo": {
    "scheme": "acme",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "icon": "src/assets/icon.png",
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "newArchEnabled": true,
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://n"
        }
      ],
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-font"
    ],
    "splash": {
      "image": "./src/assets/splash.png",
      "backgroundColor": "#3f0859",
      "resizeMode": "contain"
    },
    "name": "Otaflix",
    "slug": "Otaflix",
    "android": {
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "*.myapp.io",
              "pathPrefix": "/records"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "package": "com.yvan_2x.Otaflix"
    },
    "extra": {
      "router": {
        "origin": "https://n"
      },
      "eas": {
        "projectId": "2759ddd8-d71d-4e23-8e44-169964690e57"
      }
    },
    "ios": {
      "bundleIdentifier": "com.yvan-2x.Otaflix"
    }
  }
}
